{
  "name": "cobrowse-agent-sdk",
  "browserslist": "> 0.25%, not dead, IE 11",
  "version": "0.0.19",
  "description": "Cobrowse.io Agent SDK",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf ./dist && babel src --out-dir dist",
    "build:test": "rm -rf ./dist && babel src --source-maps inline --out-dir dist",
    "start": "babel src --out-dir dist --watch",
    "test:browser": "npm run build:test && karma start karma.conf.js",
    "test:node": "npm run build:test && mocha",
    "test": "npm run test:browser && mocha",
    "prepare": "npm run build",
    "lint": "standard",
    "docs": "typedoc src/CobrowseAPI.d.ts && rm docs/README.md && concat-md --decrease-title-levels docs > docs.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cobrowseio/cobrowse-agent-sdk"
  },
  "author": "Andy Pritchard",
  "bugs": {
    "url": "https://github.com/cobrowseio/cobrowse-agent-sdk/issues"
  },
  "homepage": "https://cobrowse.io",
  "devDependencies": {
    "concat-md": "^0.3.5",
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.16.9",
    "babel-eslint": "^10.1.0",
    "browserify": "^17.0.0",
    "eslint": "^8.7.0",
    "karma": "^6.3.12",
    "karma-browserify": "^8.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-source-map-support": "^1.4.0",
    "mocha": "^9.2.0",
    "should": "^13.2.3",
    "standard": "^16.0.4",
    "typedoc": "^0.22.11",
    "typedoc-plugin-markdown": "^3.11.12",
    "typedoc-plugin-rename-defaults": "^0.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@arduino/cbor-js": "github:cobrowseio/cbor-js",
    "core-js": "^3.20.3",
    "cross-fetch": "^3.1.5",
    "debug": "^4.3.3",
    "events": "^3.3.0",
    "lodash": "^4.17.21",
    "qs": "^6.10.3"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": [
      "browser",
      "mocha"
    ]
  }
}
