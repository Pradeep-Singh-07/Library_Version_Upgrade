{
  "name": "cobrowse-agent-sdk",
  "browserslist": "> 0.25%, not dead, IE 11",
  "version": "1.4.0",
  "description": "Cobrowse.io Agent SDK",
  "type": "module",
  "main": "dist/esm/index.js",
  "types": "src/CobrowseAPI.d.ts",
  "scripts": {
    "build": "echo export default \\'$npm_package_version\\' > ./src/version.js && git add ./src/version.js && rm -rf ./dist && babel src --out-dir dist/esm && webpack",
    "build:test": "rm -rf ./dist && babel src --source-maps inline --out-dir dist/esm && webpack",
    "start": "babel src --out-dir dist/esm --watch",
    "test:browser": "npm run build:test && karma start",
    "test:node": "mocha",
    "test": "npm run test:browser && npm run test:node",
    "prepare": "npm run build",
    "lint": "standard",
    "docs": "typedoc src/CobrowseAPI.d.ts && rm docs/README.md && concat-md --decrease-title-levels docs > docs.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cobrowseio/cobrowse-agent-sdk"
  },
  "author": "Andy Pritchard",
  "bugs": {
    "url": "https://github.com/cobrowseio/cobrowse-agent-sdk/issues"
  },
  "homepage": "https://cobrowse.io",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.19.4",
    "babel-loader": "^9.0.1",
    "concat-md": "^0.5.0",
    "karma": "^6.4.1",
    "karma-babel-preprocessor": "^8.0.2",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-source-map-support": "^1.4.0",
    "karma-webpack": "^5.0.0",
    "mocha": "^10.1.0",
    "should": "^13.2.3",
    "standard": "^17.0.0",
    "typedoc": "^0.23.19",
    "typedoc-plugin-markdown": "^3.13.6",
    "typedoc-plugin-rename-defaults": "^0.6.4",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "cbor-x": "^1.4.1",
    "cross-fetch": "^3.1.5",
    "debug": "^4.3.4",
    "events": "^3.3.0",
    "lodash": "^4.17.21",
    "qs": "^6.11.0"
  },
  "standard": {
    "env": [
      "browser",
      "mocha"
    ]
  }
}
