{
  "name": "@gnaudio/jabra-js",
  "version": "4.2.1",
  "description": "Controls Jabra devices from JavaScript. Supports both Node.js and browser.",
  "type": "module",
  "main": "./browser-esm/index.js",
  "module": "./browser-esm/index.js",
  "typings": "./browser-esm/index.d.ts",
  "files": [
    "browser-esm",
    "browser-umd",
    "node-cjs",
    "node-esm",
    "jabra-console",
    "docs",
    "package.json",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "node ./demo/device-list-node/index.js",
    "start-browser": "http-server .",
    "build": "node ./scripts/build.js",
    "build-node": "npm run build -- --target=node",
    "build-browser": "npm run build -- --target=browser",
    "bundle": "rollup -c --environment NODE_ENV:internal-development-browser",
    "clean": "node ./scripts/clean.js",
    "remove-bundles": "npm run clean -- bundles",
    "watch": "nodemon --watch ./src --exec 'npm run update-static-demo-bundle' --ext ts",
    "test": "jest",
    "test-watch": "jest --watch",
    "test-e2e": "jest --testPathPattern=e2e --verbose",
    "test-unit": "jest --testPathIgnorePatterns='e2e.*'",
    "test-coverage": "jest --coverage",
    "test-coverage-ci": "jest --ci --coverage --coverageReporters=cobertura",
    "eslint": "eslint . --ext .ts",
    "publish-private": "npm publish --access restricted",
    "changes": "node ./scripts/changes.js",
    "ci-tasks": "npm run build && npm run test && npm run eslint",
    "demo-call-control": "node --loader ts-node/esm ./demo/call-control-cli/index.ts",
    "demo-custom-gnp": "node --loader ts-node/esm ./demo/custom-gnp-node/index.ts",
    "update-jabra-console": "node ./scripts/update-jabra-console.js",
    "update-static-demo-bundle": "node ./scripts/update-static-demo-bundle.js",
    "update-api-explorer-bundle": "node ./scripts/update-api-explorer-bundle.js",
    "license": "node ./scripts/license.js",
    "docs": "node ./scripts/docs.js",
    "sample-acquire-call-lock": "node --loader ts-node/esm ./documentation/code-samples/dev-guide/acquire-release-call-lock.ts",
    "sample-acc-connection-changes": "node --loader ts-node/esm ./documentation/code-samples/dev-guide/acc-connection-changes.ts",
    "sample-call-lock-full": "node --loader ts-node/esm ./documentation/code-samples/dev-guide/call-lock-full-example.ts",
    "sample-device-connection-changes": "node --loader ts-node/esm ./documentation/code-samples/dev-guide/device-connection-changes.ts",
    "sample-device-list-changes": "node --loader ts-node/esm ./documentation/code-samples/dev-guide/device-list-changes.ts",
    "sample-ecc-init-multi": "node --loader ts-node/esm ./documentation/code-samples/dev-guide/ecc-initial-state-multi-call.ts",
    "sample-ecc-init-single": "node --loader ts-node/esm ./documentation/code-samples/dev-guide/ecc-initial-state-single-call.ts",
    "sample-full-device-connection": "node --loader ts-node/esm ./documentation/code-samples/dev-guide/full-device-and-connection.ts",
    "sample-init-acc": "node --loader ts-node/esm ./documentation/code-samples/dev-guide/initialise-acc.ts",
    "sample-init-ecc-multi": "node --loader ts-node/esm ./documentation/code-samples/dev-guide/initialise-ecc-multi-call.ts",
    "sample-init-ecc-single": "node --loader ts-node/esm ./documentation/code-samples/dev-guide/initialise-ecc-single-call.ts",
    "sample-print-connections-and-subscribe": "node --loader ts-node/esm ./documentation/code-samples/dev-guide/print-connections-and-subscribe-to-changes.ts",
    "sample-print-devices-and-subscribe": "node --loader ts-node/esm ./documentation/code-samples/dev-guide/print-devices-and-subscribe-to-changes.ts",
    "assemble": "node ./scripts/assemble.js",
    "//": "lifecycle scripts",
    "prepack": "node ./scripts/pack.js pre public",
    "postpack": "node ./scripts/pack.js post public"
  },
  "author": "GN Audio A/S",
  "license": "SEE LICENSE IN: LICENSE",
  "devDependencies": {
    "@microsoft/api-documenter": "^7.13.12",
    "@microsoft/api-extractor": "^7.15.2",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/docopt": "^0.6.33",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.1",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "concat-licenses": "^3.0.1",
    "conventional-commits-parser": "^3.2.2",
    "docopt": "^0.6.2",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "fs-extra": "^7.0.1",
    "git-raw-commits": "^2.0.10",
    "git-semver-tags": "^4.1.1",
    "http-server": "^14.1.1",
    "inquirer": "^7.3.3",
    "jest": "^26.6.3",
    "jest-trx-results-processor": "^2.2.0",
    "moq.ts": "^6.4.0",
    "nodemon": "^2.0.7",
    "replace-in-file": "^6.2.0",
    "rollup": "^2.50.5",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typescript": "~4.3.2"
  },
  "//": "If you add any packages here, make sure to inspect the changes to LICENSE after building!",
  "dependencies": {
    "bit-buffer": "^0.2.5",
    "pure-uuid": "1.6.1",
    "rxjs": "^7.1.0"
  }
}
